

@model List<AspCore.WebComponents.ViewComponents.Alert.Concrete.Alert.AlertInfo>;

<script>
         @if (Model != null)
         {
               @foreach (var alertInfo in Model)
                {

                   @:Alert.showAlert('@alertInfo.Type', '@alertInfo.Title', '@alertInfo.Message','@alertInfo.AlertType.GetHashCode()');

                }
         }
</script>


